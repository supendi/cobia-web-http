{
  "name": "@cobia/http",
  "version": "1.0.0",
  "description": "The Cobia API Http client",
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf ./dist",
    "compile": "tsc",
    "build": "yarn clean && yarn compile",
    "remove-cobia": "yarn remove @cobia/types",
    "build-types": "cd ../cobia-types-ts && yarn build",
    "add-cobia": "yarn add ../cobia-types-ts",
    "update-cobia": "yarn build-types && yarn remove-cobia && yarn add-cobia",
    "clean-node-modules": "rm -rf ./node_modules && rm yarn.lock"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supendi/cobia-web-http.git"
  },
  "author": "Supendi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/supendi/cobia-web-http/issues"
  },
  "homepage": "https://github.com/supendi/cobia-web-http#readme",
  "dependencies": {
    "@cobia/types": "../cobia-types-ts",
    "axios": "^0.21.1"
  },
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@types/node": "^14.14.41"
  }
}
