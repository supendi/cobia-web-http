{
  "name": "@cobia/http",
  "version": "1.0.0",
  "description": "The Cobia API Http client",
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf ./dist",
    "compile": "tsc",
    "clean-compile": "yarn clean && yarn compile",
    "remove-cobia": "yarn remove @cobia/types && yarn remove @cobia/lib",
    "compile-lib": "cd ../cobia-web-lib && yarn clean-compile",
    "compile-types": "cd ../cobia-types-ts && yarn clean-compile",
    "add-cobia": "yarn add ../cobia-types-ts && yarn add ../cobia-web-lib",
    "update-cobia": "yarn compile-lib && yarn compile-types && yarn remove-cobia && yarn add-cobia",
    "clean-node-modules": "rm -rf ./node_modules && rm yarn.lock"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supendi/cobia-web-http.git"
  },
  "author": "Supendi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/supendi/cobia-web-http/issues"
  },
  "homepage": "https://github.com/supendi/cobia-web-http#readme",
  "dependencies": {
    "@cobia/lib": "../cobia-web-lib",
    "@cobia/types": "../cobia-types-ts",
    "axios": "^0.21.1"
  },
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@types/node": "^14.14.41"
  }
}
